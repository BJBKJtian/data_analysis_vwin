import pandas as pd

# 函数说明
# pandas.read_excel(io,sheet_name=0,header=0,names=None,index_col=None,usecols=None,...)
# io：文件路径
# sheet_name：指定sheet页；  例：sheet_name=[0,1,'sheet3']
# header：指定列名，默认为0，即第一行
# names：指定列名，人为赋值；  例：names=['name','gender']
# index_col：指定索引列
# usecols：指定提取哪些列；  例：usecols=[0,.3]

data = pd.read_excel(r"C:\Users\Administrator\Desktop\test.xlsx")
